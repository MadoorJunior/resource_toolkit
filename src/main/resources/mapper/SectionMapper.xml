<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.madoor.resource_toolkit.mapper.SectionMapper">
    <select id="getIdByName" resultType="integer">
        select period_id from t_period where period_name = #{name}
    </select>
</mapper>